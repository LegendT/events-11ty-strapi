{% extends "layouts/base.html" %}
{% set pageHeaderTitle = title %}
{% set
pageHeaderSummary = summary %}
{% block
content %}
    <div class="wrapper flow">
        {# <article>{% include "partials/page-header.html" %}</article> #}
        <ol class="[ schedule-list ]">
            <li class="schedule-list__item bg-light-shade">
                <h2>Registration</h2>
				<p class=" [ schedule-list__content-time ] [ text-400 weight-bold measure-short flow-space-300 ] "><span class="visually-hidden">Time: </span> 08:45am - 09:10am</p>
            </li>
			<li class="schedule-list__item bg-light-shade">
                <h2>Opening Address - Simon Norris</h2>
				<p class=" [ speaker-list__summary ] "><span class="visually-hidden">Job title: </span>CEO Nomensa</p>
				<p class=" [ schedule-list__content-time ] [ text-400 weight-bold measure-short flow-space-300 ] "><span class="visually-hidden">Time: </span> 09:15am - 09:30am</p>
            </li>
            <li class="schedule-list__item">
                <article class="[ radius-panel ]" data-variant="horizontal">
                    <figure class="[ radius-panel__item ] [ auto-grid ]">
                        <div class="[ radius-panel__media ]">
                            <img class="radius" src="{{ speaker[0].data.image }}" alt="{{ speaker[0].data.imageAlt }}"/>
                        </div>
                        <figcaption class="[ radius-panel__content ]">
                            <h2 class="[ text-600 ]">
                                <span class="visually-hidden">Name:
                                </span>
                                {{ speaker[0].data.title }}
                            </h2>
                            <p class=" [ speaker-list__summary ] ">
                                <span class="visually-hidden">Job title:
                                </span>
                                {{ speaker[0].data.summary }}
                            </p>
                            <p class=" [ schedule-list__content-time ] [ text-400 weight-bold measure-short flow-space-300 ] ">
                                <span class="visually-hidden">Talk time
                                </span>
                                {{ speaker[0].talkTimeStart }} - {{ speaker[0].talkTimeEnd }}
                            </p>
                            {{ speaker[0].data.talkDetails }}
                            <a href="/speaker/{{ speaker[0].data.title | slug }}/" class="card-link">
                                <span class="visually-hidden">read more about {{ speaker[0].data.title }}</span>
                            </a>
                        </figcaption>
                    </figure>
                </article>
            </li>
